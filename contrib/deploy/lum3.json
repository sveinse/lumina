{
    "plugins": ["telldus"],

    "server": "hus.local",
    "port": 5326,

    "telldus.config": [

        // Telldus output devices -- must be synced with tellstick.conf to work
        // name must contain a {op} field
        { "type": "dimmer", "id": 100, "house":  4785058, "unit": 1, "name": "light/{op}" },
        { "type": "dimmer", "id": 101, "house": 83621799, "unit": 1, "name": "light/roof/{op}" },
        { "type": "dimmer", "id": 102, "model": 86669189, "unit": 2, "name": "light/pj/{op}" },
        { "type": "dimmer", "id": 103, "model": 11630920, "unit": 2, "name": "light/sceen/{op}" },
        { "type": "dimmer", "id": 104, "model": 50026144, "unit": 2, "name": "light/wall/{op}" },
        { "type": "dimmer", "id": 105, "model": 27361582, "unit": 1, "name": "light/table/{op}" },
        { "type": "switch", "id": 106, "model": 12052346, "unit": 1, "name": "ledpwr/{op}" },

        // Telldus input switches. name must contain a {method} field, and either an unit
        // attribute or num_units.
        { "type": "in", "house": 14244686, "group": 1, "unit": 1,       "name": "remote/g/{method}" },
        { "type": "in", "house": 14244686, "group": 0, "num_units": 16, "name": "remote/{unit}/{method}" },
        { "type": "in", "house": 366702,   "group": 0, "unit": 1,       "name": "wallsw1/{method}"  },
        { "type": "in", "house": 392498,   "group": 0, "unit": 1,       "name": "wallsw2/{method}"  }

        // Temperature devices
        //{ "type": "temp", "id": 11,  "name": "temp/ute" },
        //{ "type": "temp", "id": 12,  "name": "temp/kjeller"  },
        //{ "type": "temp", "id": 247, "name": "temp/fryser"  },
        //{ "type": "temp", "id": 135, "name": "temp/kino/ute"  },
        //{ "type": "temp", "id": 151, "name": "temp/kino/inne"  }

    ]
}
